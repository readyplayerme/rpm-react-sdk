{
  "name": "@readyplayerme/rpm-react-sdk",
  "version": "0.1.4-rc.0",
  "description": "Ready Player Me React SDK",
  "keywords": [
    "readyplayerme",
    "avatar",
    "react",
    "typescript",
    "glb"
  ],
  "license": "MIT",
  "author": "Ready Player Me",
  "main": "dist/index.js",
  "module": "dist/rpm-web-sdk.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "build-storybook": "storybook build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "storybook": "storybook dev -p 6006",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 400,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/rpm-web-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rpm-web-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/addons": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-vite": "^7.0.8",
    "@tsconfig/recommended": "^1.0.2",
    "@tsconfig/vite-react": "^1.0.1",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "babel-loader": "^9.1.2",
    "dts-cli": "^2.0.2",
    "husky": "^8.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^18.2.0",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.8",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@readyplayerme/visage": "^3.5.2"
  }
}